{
    "content_scripts":[
        {
            "matches":["https://github.com/*"],
            "js":[
                "js/lib/jquery-171.js",
                "js/lib/underscore-min.js",
                "js/lib/jquery-ui-1.8.20.custom.min.js",
                "js/lib/event.js",
                "js/lib/util.js",
                "js/widgets.js",
                "js/github_content.js",
                "js/user_creds.js"
            ],
            "css": [ "style.css" ],
            "run_at":"document_end"
        },
        {
            "matches":["http://gimply.herokuapp.com/login_success*"],
            "js": [ "js/oauth_token_reader.js"],
            "run_at": "document_start"
        }
    ],
    "background":{
        "page":"index.html"
    },
    "description":"Updates from a Git Project",
    "name":"Gimply",
    "permissions":["https://api.github.com/", "unlimitedStorage", "notifications"],
    "version":"0.0.1"
}
